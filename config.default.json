{
    "connections": [
        {
            "name": "esper",
            "type": "irc",
            "auto_rejoin": false,
            "invite_join": true,
            "join_throttle": 0.4,
            "require_registered_channels": false,
            "connection": {
                "server": "irc.esper.net",
                "port": 6667,
                "ssl": false,
                "ignore_cert": true,
                "password": "",
                "timeout": 300,
                "client_cert": "cloudbot.pem",
                "bind_addr": "",
                "bind_port": 0
            },
            "ping_settings": {
                "interval": 60,
                "warn": 120,
                "timeout": 300
            },
            "user_agent": "CloudBot/3.0 - CloudBot Refresh <https://github.com/TotallyNotRobots/CloudBot/>",
            "reply_ping": true,
            "nick": "MyCloudBot",
            "user": "cloudbot",
            "realname": "CloudBot - https://git.io/CloudBot",
            "avoid_notices": false,
            "channels": [
                "#cloudbot",
                "#cloudbot2"
            ],
            "mode": null,
            "oper_user": null,
            "oper_pw": null,
            "log_channel": "#MyAdminChannel",
            "disabled_commands": [],
            "acls": {},
            "sasl": {
                "enabled": false,
                "mechanism": "PLAIN",
                "user": "",
                "pass": ""
            },
            "nickserv": {
                "enabled": false,
                "nickserv_password": "",
                "nickserv_user": "",
                "nickserv_name": "nickserv",
                "nickserv_command": "IDENTIFY"
            },
            "strip_newlines": true,
            "max_line_length": 510,
            "encoding": "utf-8",
            "encoding_errors": "replace",
            "strip_cmd_chars": "!.@;$",
            "ratelimit": {
                "max_tokens": 17.5,
                "restore_rate": 2.5,
                "message_cost": 5,
                "strict": true
            },
            "permissions": {
                "admins": {
                    "perms": [
                        "addfactoid",
                        "delfactoid",
                        "ignore",
                        "botcontrol",
                        "plpaste",
                        "permissions_users",
                        "op"
                    ],
                    "users": [
                        "examplea!user@example.com",
                        "exampleb!user@example.com"
                    ]
                },
                "moderators": {
                    "perms": [
                        "addfactoid",
                        "delfactoid",
                        "ignore"
                    ],
                    "users": [
                        "examplec!user@example.com"
                    ]
                }
            },
            "plugins": {},
            "command_prefix": "."
        }
    ],
    "plugins": {
        "duckhunt": {
            "minimum_messages": 10,
            "minimum_users": 5
        }
    },
    "api_keys": {
